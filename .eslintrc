{
    "parser": "babel-eslint",
    "settings": {
        "react": {
            "version": "16.4"
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": ["error", {"objects": "always-multiline"}],
        "eol-last": ["error", "always"],
        "indent": ["error", 4],
        "max-len": [1, 120, 2, {"ignoreComments": true}],
        "react/prop-types": [2]
    }
}